{
  "name": "crypto-price",
  "manifest_version": 3,
  "version": "0.1",
  "description": "This extension gives you price of BTC, ETH, SOL and LINK ",
  "background": {
    "service_worker": "main.js"
  },
  "action": {
    "default_popup": "/scripts/index.html"
  },
  "icons": {
    "16": "/images/icon.png",
    "32": "/images/icon.png",
    "48": "/images/icon.png",
    "128": "/images/icon.png"
  }
}
