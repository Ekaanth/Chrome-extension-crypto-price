{
  "name": "crypto-price",
  "manifest_version": 3,
  "version": "0.1",
  "description": "This extension gives you price of BTC, ETH, SOL and LINK ",
  "action": {
    "default_popup": "index.html"
  },
  "icons": {
    "192": "logo192.png",
    "512": "logo512.png"
  },
  "content_security_policy": {
    "extension_pages": "script-src 'self'; object-src 'self'"
  }
}
